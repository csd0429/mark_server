	               u'\u4e0d\u53ef\u5426\u8ba4\uff0c\u963f\u91cc\u662f\u201c\u53cc\u5341\u4e00\u201d\u7684\u521b\u9020\u8005\uff0c2009\u5e74\u963f\u91cc\u5c06\u201c\u5149\u68cd\u8282\u201d\u6253\u9020\u6210\u201c\u8d2d\u7269\u8282\u201d\uff0c\u4ece\u6b64\u6539\u53d8\u4e86\u7535\u5546\u7684\u5e74\u5e95\u4fc3\u9500\u65b9\u6848\uff0c\u4e5f\u6539\u53d8\u4e86\u6d88\u8d39\u8005\u7684\u8d2d\u7269\u4e60\u60ef\u3002\u963f\u91cc\u201c\u53cc\u5341\u4e00\u201d\u7684\u9500\u552e\u989d\uff0c\u4ece2009\u5e74\u76841\u4ebf\u5143\uff0c\u98d9\u5347\u81f32013\u5e74\u7684350\u4ebf\u5143\u3002\u4e0e\u6b64\u540c\u65f6\uff0c\u4eac\u4e1c\u3001\u82cf\u5b81\u7b49\u7535\u5546\u4e5f\u52a0\u5165\u5176\u4e2d\uff0c\u6700\u7ec8\u5f62\u6210\u4e86\u5168\u7403\u77a9\u76ee\u7684\u201c\u53cc\u5341\u4e00\u201d\u73b0\u8c61\u3002',
	               u'2009\u5e74\uff0c\u201c\u53cc\u5341\u4e00\u201d\u5f53\u65e5\u652f\u4ed8\u5b9d\u4ea4\u6613\u989d\u7ea6\u4e3a1\u4ebf\u5143;\u52302010\u5e74\uff0c\u589e\u957f\u52309.36\u4ebf\u5143;2011\u5e74\uff0c\u589e\u957f\u523052\u4ebf\u5143\u30022012\u5e74\uff0c\u201c\u53cc\u5341\u4e00\u201d\u5f53\u65e5\u652f\u4ed8\u5b9d\u4ea4\u6613\u989d\u5b9e\u73b0\u98de\u901f\u589e\u957f\uff0c\u8fbe\u5230191\u4ebf\u5143\uff0c\u5176\u4e2d\u5305\u62ec\u5929\u732b\u5546\u57ce132\u4ebf\u5143\uff0c\u6dd8\u5b9d59\u4ebf\u5143;2013\u5e74\uff0c\u201c\u53cc\u5341\u4e00\u201d\u4fc3\u9500\u6d3b\u52a8\u603b\u6210\u4ea4\u989d\u8d85\u8fc7350\u4ebf\u5143\uff0c\u5176\u4e2d\uff0c',
	               u'\u624b\u673a',
	               u'\u6dd8\u5b9d\u9500\u552e\u989d\u4e3a53.5\u4ebf\u5143\uff0c\u652f\u4ed8\u5b9d\u8fbe\u6210\u7684\u4ea4\u6613\u7b14\u6570\u4e3a1.7\u4ebf\u7b14\u3002\u53bb\u5e74\u201c\u53cc\u5341\u4e00\u201d\u6dd8\u5b9d\u4e00\u5929\u6210\u4ea4\u989d\u9ad8\u8fbe351\u4ebf\u5143\uff0c\u4e1a\u5185\u4eba\u58eb\u9884\u8ba1\uff0c\u4eca\u5e74\u201c\u53cc\u5341\u4e00\u201d\u9500\u552e\u989d\u4e0a500\u4ebf\u5143\u95ee\u9898\u4e0d\u5927\uff0c\u6709\u671b\u51b2\u51fb600\u4ebf\u5143\u3002',
	               u'\u201c\u53cc\u5341\u4e00\u201d\u8d2d\u7269\u72c2\u6b22\u8282',
	               u'\u5728\u770b\u597d\u201c\u53cc\u5341\u4e00\u201d\u8fd9\u4e2a\u5e02\u573a\u4e4b\u540e\uff0c\u5404\u5bb6\u7535\u5546\u90fd\u5728\u79ef\u6781\u5907\u6218\uff0c\u4f46\u4eca\u5e74\u60c5\u51b5\u4f3c\u4e4e\u4e0e\u5f80\u5e74\u4e0d\u540c\u3002\u963f\u91cc\u62a2\u6ce8\u201c\u53cc\u5341\u4e00\u201d\u5546\u6807\uff0c\u53d1\u51fd\u8981\u6c42\u5a92\u4f53\u520a\u53d1\u5176\u4ed6\u7535\u5546\u4fc3\u9500',
	               u'\u5e7f\u544a',
	               u'\u65f6\u7981\u6b62\u4f7f\u7528\u201c\u53cc\u5341\u4e00\u201d\u5b57\u6837\u3002\u6b64\u4e3e\u7acb\u5373\u5f15\u53d1\u4e86\u4eac\u4e1c\u82cf\u5b81\u4e0d\u6ee1\u3002\u4eac\u4e1c\u6307\u963f\u91cc\u201c\u501f\u6cd5\u5f8b\u4e4b\u540d\uff0c\u884c\u5784\u65ad\u4e4b\u5b9e\u201d\uff0c\u82cf\u5b81\u4e5f\u79f0\u201c\u6cd5\u5f8b\u5408\u89c4\u624b\u6bb5\u4e0d\u4e49\u201d\u3002\u867d\u7136\u4e0d\u6ee1\uff0c\u4f46\u8fd8\u662f\u4e0d\u5f97\u4e0d\u5c06\u201c\u53cc\u5341\u4e00\u201d\u5b57\u6837\u62b9\u53bb\uff0c\u4eac\u4e1c\u6539\u4e3a\u201c11.11\u201d\u3002',
	               u'\u7ee7\u53bb\u5e74\u5929\u732b\u5728\u201c\u53cc\u5341\u4e00\u201d\u524d\u540e\u521b\u9020\u51fa\u8d85\u8fc7350\u4ebf\u5143\u7684\u9500\u552e\u989d\u540e\uff0c\u7535\u5546\u90fd\u7784\u51c6\u4e86\u201c\u53cc\u5341\u4e00\u201d\u8fd9\u4e00\u8282\u70b9\u5e26\u6765\u7684\u9500\u552e\u6f5c\u529b\uff0c\u5404\u5927\u7535\u5546\u6469\u62f3\u64e6\u638c\uff0c\u79ef\u6781\u5907\u6218\u3002\u4f5c\u4e3a\u963f\u91cc\u5df4\u5df4\u4e0a\u5e02\u540e\u9996\u4e2a\u201c\u53cc\u5341\u4e00\u201d\uff0c\u4eca\u5e74\u805a\u7126\u4e8e\u5168\u7403\u5316\uff0c\u53c2\u4e0e\u5546\u5bb6\u7684\u6570\u91cf\u521b\u65b0\u9ad8\uff0c\u603b\u8ba1\u8fbe\u52302.7\u4e07\u5bb6\uff0c\u8ba9\u6d88\u8d39\u8005\u5728\u5bb6\u5c31\u80fd\u201c\u8d2d\u5168\u7403\u201d\u3002\u963f\u91cc\u52b2\u654c\u4e4b\u4e00\u4eac\u4e1c\u5219\u63a8\u51fa\u4e86\u201c\u4e00\u4ef7\u5230\u5e9512\u5929\u201d\u6d3b\u52a8\uff0c\u627f\u8bfa\u53c2\u4e0e\u4fc3\u9500\u6d3b\u52a8\u7684\u54c1\u724c\u572811\u67081\u65e5-9\u65e5\u7684\u4ef7\u683c\u7edd\u4e0d\u4f1a\u9ad8\u4e8e10\u65e5-12\u65e5\u7684\u8ba2\u5355\u9ad8\u6f6e\u671f\u4ef7\u683c\u3002',
	               u'\u5f53\u5f53\u7f51\u81ea\u4ece\u66f4\u540d\u201c\u5f53\u5f53\u201d\u540e\uff0c\u5176LOGO\u4e5f\u53d8\u6210\u4e00\u53ea\u7334\u5b50\u3002\u4eca\u5e74\u201c\u53cc\u5341\u4e00\u201d\u524d\u5915\uff0c\u5f53\u5f53\u5ba3\u5e03\u5728\u4eca\u5e74\u4e0d\u6b62\u4e94\u6298\uff0c\u8981\u4e0e\u5929\u732b\u3001\u4eac\u4e1c\u6765\u573a\u201c\u732b\u72d7\u7334\u201d\u5927\u6218\u3002\u4e3a\u4e86\u8fce\u63a5\u201c\u53cc\u5341\u4e00\u201d\u8fd9\u4e00\u9500\u552e\u65fa\u5b63\uff0c\u82cf\u5b81\u65e9\u572810\u670827\u65e5\u5c31\u5f00\u59cb\u4fc3\u9500\u6d3b\u52a8\uff0c\u6301\u7eed\u523011\u6708\u521d\uff0c\u5e76\u79f0\u8981\u201c\u653e\u5f00\u9152\u91cf\u4f46\u4e0d\u53d1\u75af\u201d\u3002',
	               u'\u201c\u53cc\u5341\u4e00\u201d\u7ea2\u706b\u7684\u80cc\u540e\uff0c\u662f\u6d88\u8d39\u8005\u8d2d\u7269\u4e60\u60ef\u7684\u53d8\u8fc1\u3002\u827e\u745e\u6570\u636e\u663e\u793a\uff0c2009\u5e74\u4e2d\u56fd',
	               u'\u7f51\u7edc\u8d2d\u7269',
	               u'\u5e02\u573a\u89c4\u6a21\u4e0d\u52302500\u4ebf\u5143\u3002\u5230\u4e862013\u5e74\uff0c\u8fd9\u4e00\u91d1\u989d\u589e\u81f31.85\u4e07\u4ebf\u5143\uff0c\u662f2009\u5e74\u76847.4\u500d\u3002\u56fd\u5bb6\u7edf\u8ba1\u5c40\u6570\u636e\u663e\u793a\uff0c\u5728\u6b64\u671f\u95f4\uff0c\u793e\u4f1a\u6d88\u8d39\u54c1\u96f6\u552e\u603b\u989d\u589e\u81f3\u4e0d\u52301.8\u500d\u3002\u5feb\u6162\u4e0d\u5747\u4e4b\u4e0b\uff0c\u4eca\u5e74\u4e8c\u5b63\u5ea6\uff0c\u7f51\u7edc\u8d2d\u7269\u5728\u793e\u4f1a\u6d88\u8d39\u54c1\u96f6\u552e\u603b\u989d\u4e2d\u7684\u5360\u6bd4\u9996\u6b21\u5355\u5b63\u5ea6\u7a81\u783410%\u3002',
	               u'\u5de5\u5546\u603b\u5c40\u63d0\u524d\u6253\u9884\u9632\u9488',
	               u'\u5c31\u5728\u521a\u521a\u8fc7\u53bb\u7684\u8fd9\u4e2a\u5468\u672b\uff0c\u5de5\u5546\u603b\u5c40\u53ec\u96c6\u4e8610\u5bb6\u7535\u5546\u4f01\u4e1a\u8fdb\u884c\u7ea6\u8c08\uff0c\u4e3b\u8981\u56f4\u7ed5\u5982\u4f55\u5bf9\u201c\u9500\u552e\u884c\u4e3a\u7ba1\u7406\u6df7\u4e71\u201d\u7684\u95ee\u9898\u5c55\u5f00\u3002\u4e2d\u534e\u5de5\u5546\u65f6\u62a5\u8bb0\u8005\u4ece\u5de5\u5546\u603b\u5c40\u83b7\u6089\uff0c\u53c2\u52a0\u6b64\u6b21\u7ea6\u8c08\u768410\u5bb6\u7535\u5546\u5206\u522b\u4e3a\uff0c\u963f\u91cc\u5df4\u5df4\u3001\u4eac\u4e1c\u3001\u552f\u54c1\u4f1a\u3001\u82cf\u5b81\u6613\u8d2d\u30011\u53f7\u5e97\u3001\u4e9a\u9a6c\u900a\u5546\u57ce\u3001\u805a\u7f8e\u4f18\u54c1\u3001\u643a\u7a0b\u300158\u540c\u57ce\u3001\u53bb\u54ea\u513f\u7f51\u3002',
	               u'\u5de5\u5546\u603b\u5c40\u526f\u5c40\u957f\u7518\u9716\u5728\u7ea6\u8c08\u4f1a\u4e0a\u8868\u793a\uff0c\u8981\u9ad8\u5ea6\u91cd\u89c6\u5e76\u7ea0\u6b63\u7f51\u7edc\u5546\u54c1\u4ea4\u6613\u4e2d\u5b58\u5728\u7684\u7b2c\u4e09\u65b9\u4ea4\u6613\u5e73\u53f0\u51c6\u5165\u628a\u5173\u4e0d\u4e25\u3001\u5bf9\u5546\u54c1\u4fe1\u606f\u7684\u5ba1\u67e5\u5904\u7f6e\u4e0d\u529b\u3001\u6d88\u8d39\u8005\u6743\u76ca\u672a\u80fd\u5f97\u5230\u5145\u5206\u4fdd\u62a4\u3001\u7f51\u7edc\u7ecf\u8425\u6570\u636e\u8fc7\u5ea6\u5c01\u95ed\u548c\u9500\u552e\u884c\u4e3a\u7ba1\u7406\u6df7\u4e71\u7b49\u65b9\u9762\u7684\u8fdd\u6cd5\u8fdd\u89c4\u884c\u4e3a\u3002',
	               u'\u201c\u5c24\u5176\u662f\u5207\u5b9e\u7ea0\u6b63\u5047\u6253\u6298\u8bef\u5bfc\u6d88\u8d39\u8005\u3001\u865a\u5047\u5ba3\u4f20\u53ca\u8fdd\u6cd5\u5e7f\u544a\u3001\u4e0d\u516c\u5e73\u683c\u5f0f\u6761\u6b3e\u548c\u4e0d\u5408\u7406\u9644\u52a0\u8981\u6c42\u3001\u865a\u5047\u4fe1\u7528\u8bc4\u4ef7\u7b49\u95ee\u9898\u3002\u201d\u7518\u9716\u79f0\u3002',
	               u'\u5de5\u5546\u603b\u5c40\u8981\u6c42\uff0c\u7535\u5546\u8981\u91c7\u53d6\u516c\u5f00\u4fc3\u9500\u89c4\u5219\u3001\u5982\u5b9e\u6807\u660e\u4ef7\u683c\u3001\u4fdd\u969c\u5546\u54c1\u8d28\u91cf\u3001\u5b8c\u5584\u6280\u672f\u548c',
	               u'\u7269\u6d41',
	               u'\u4fdd\u969c\u3001\u5236\u5b9a\u6295\u8bc9\u5904\u7406\u9884\u6848\u7b49\u5177\u4f53\u63aa\u65bd\uff0c\u5728\u4fc3\u9500\u6d3b\u52a8\u4e2d\u9500\u552e\u3001\u9644\u8d60\u7684\u5546\u54c1\u987b\u7b26\u5408\u300a\u4ea7\u54c1\u8d28\u91cf\u6cd5\u300b\u7684\u89c4\u5b9a\uff0c\u4e0d\u5f97\u56e0\u4fc3\u9500\u964d\u4f4e\u5546\u54c1\u8d28\u91cf\u3002\u6b64\u5916\uff0c\u786e\u4fdd\u201c7\u65e5\u65e0\u7406\u7531\u9000\u8d27\u201d\u7b49\u6cd5\u5f8b\u89c4\u5b9a\u5f97\u5230\u5207\u5b9e\u6267\u884c\u3002'],
