	               u'\u6839\u636e\u7b2c\u4e09\u65b9\u5206\u6790\u673a\u6784bitcoinity.org\u7684\u6570\u636e\uff08\u672a',
	               u'\u76d1\u63a7',
	               u'OKCoin\uff09\uff0c2013\u5e749\u6708\u81f3\u4eca\uff0c\u706b\u5e01\u7f51\u548c\u6bd4\u7279\u5e01\u4e2d\u56fd\u7d2f\u8ba1\u6bd4\u7279\u5e01\u603b\u4ea4\u6613\u91cf\u5206\u522b\u5360\u5e02\u573a\u4efd\u989d\u768426.37%\u548c21.33%\uff0c\u6392\u540d\u5168\u7f51\u7b2c\u4e00\u3001\u7b2c\u4e8c\u3002\u8fd9\u6837\u7684\u89c4\u6a21\u7740\u5b9e\u4ee4\u8bb8\u591a\u91d1\u878d\u754c\u7684\u4eba\u58eb\u90fd\u611f\u5230\u60ca\u8bb6\uff0c\u5c24\u5176\u662f\u4e2d\u56fd\u76d1\u7ba1\u673a\u5173\u63d0\u793a\u6bd4\u7279\u5e01\u98ce\u9669\u540e\u4e0d\u4e45\uff0c\u5883\u5185\u6bd4\u7279\u5e01\u4ea4\u6613\u7f51\u7ad9\u5e76\u672a\u53d7\u5230\u8fc7\u591a\u7684\u51b2\u51fb\uff0c\u8fc5\u901f\u8d70\u51fa\u4e86\u9634\u973e\u3002',
	               u'\u524d\u77bb\u70b9\u8bc4',
	               u'\uff1a',
	               u'\u6cd5\u5b9a\u8d27\u5e01\uff0c\u662f\u4f9d\u9760\u653f\u5e9c\u7684\u6cd5\u4ee4\u4f7f\u5176\u6210\u4e3a\u5408\u6cd5\u901a\u8d27\u7684\u8d27\u5e01\u3002\u800c\u201c\u53bb\u4e2d\u5fc3\u5316\u201d\u7684\u6bd4\u7279\u5e01\uff0c\u5374\u5728\u6311\u6218\u6cd5\u5b9a\u8d27\u5e01\u7684\u6743\u5a01\uff0c\u6270\u4e71\u4e86\u6cd5\u5b9a\u8d27\u5e01\u7684\u5408\u6cd5\u5730\u4f4d\u3002',
