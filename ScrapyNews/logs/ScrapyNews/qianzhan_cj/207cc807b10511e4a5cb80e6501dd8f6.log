	               u'\u4e3a\u4ec0\u4e48\u4f1a\u6709\u8fd9\u4e48\u591a\u4e2d\u56fd\u4eba\u4e58\u5750\u8fd9\u6b21\u97e9\u56fd\u822a\u7a7a\u516c\u53f8\u7684\u822a\u73ed?',
	               u'\u636e\u524d\u77bb\u7f51\u4e86\u89e3\uff0c\u5931\u4e8b\u7684\u97e9\u56fd\u5ba2\u673a\u8fd1\u4e00\u534a\u4e3a\u4e2d\u56fd\u5ba2\u4eba\u7684\u80cc\u540e\uff0c\u662f\u5305\u62ec\u97e9\u4e9a\u822a\u7a7a\u5728\u5185\u7684\u97e9\u56fd\u822a\u7a7a\u516c\u53f8\u8fd1\u4e9b\u5e74\u5728\u4e2d\u56fd\u5e02\u573a\u7684\u5f00\u62d3\u3002',
	               u'\u6b64\u524d\uff0c\u97e9\u4e9a\u822a\u7a7aCEO\u5c39\u6c38\u6597\u8868\u793a\uff0c\u4e2d\u56fd\u548c',
	               u'\u65e5\u672c',
	               u'\u5206\u522b\u662f\u97e9\u4e9a\u822a\u7a7a\u5168\u7403\u7684\u6700\u5927\u5e02\u573a\u53ca\u6700\u4e3b\u8981\u5229\u6da6\u6765\u6e90\uff0c\u800c\u5c31\u5e02\u573a\u6f5c\u529b\u548c\u589e\u901f\u6765\u770b\uff0c\u4e2d\u56fd\u4ecd\u662f\u5176\u62bc\u5b9d\u7684\u91cd\u70b9\u3002',
	               u'\u6628\u65e5\uff0c\u4e00\u4f4d\u822a\u7a7a\u754c\u8d44\u6df1\u4eba\u58eb\u8868\u793a\uff0c\u97e9\u4e9a\u822a\u7a7a\u8fd1\u51e0\u5e74\u5728\u4e2d\u56fd\u5185\u5730\u53d1\u5c55\u8fc5\u901f\uff0c\u5176\u7ade\u4e89\u529b\u5728\u4e8e\u4e09\u65b9\u9762\uff1a\u4ef7\u683c\u3001\u9500\u552e\u7f51\u7edc\u4ee5\u53ca\u900f\u660e\u6613\u4e8e\u67e5\u8be2\u7684\u4fe1\u606f\u6280\u672f\u3002',
	               u'\u7f51\u70b9\u94fa\u81f3\u4e8c\u4e09\u7ebf\u57ce\u5e02',
	               u'\u97e9\u4e9a\u822a\u7a7a\u8868\u793a\uff0c\u8be5\u822a\u73ed\u4e58\u5ba2\u5171291\u4eba\uff0c\u5176\u4e2d\u6709\u4e2d\u56fd\u7c4d141\u4eba\uff0c\u97e9\u56fd\u7c4d77\u4eba\uff0c\u7f8e\u56fd\u7c4d64\u4eba\uff0c\u5370\u5ea6\u7c4d3\u4eba\uff0c\u52a0\u62ff\u5927\u7c4d3\u4eba\uff0c\u6cd5\u56fd\u7c4d1\u4eba\uff0c\u65e5\u672c\u7c4d1\u4eba\uff0c\u8d8a\u5357\u7c4d1\u4eba\u3002',
	               u'\u97e9\u56fd\u7684\u5931\u4e8b\u98de\u673a\u4e0a\uff0c\u4e3a\u4f55\u8fd1\u534a\u6570\u90fd\u662f\u4e2d\u56fd\u65c5\u5ba2?\u6709\u5a92\u4f53\u63f4\u5f15\u4e1a\u5185\u4eba\u58eb\u6d88\u606f\u79f0\uff0c\u8fd9\u4e3b\u8981\u662f\u7531\u4e8e\u7531\u97e9\u56fd\u8f6c\u673a\u8d74\u7f8e\u7684\u822a\u73ed\u7968\u4ef7\u8f83\u4f4e\uff0c\u73ed\u6b21\u8f83\u591a\uff0c\u4e14\u4ece\u4eca\u5e745\u67081\u65e5\u8d77\uff0c\u6301\u8d74\u7f8e\u7b7e\u8bc1\u7684\u4e2d\u56fd\u6e38\u5ba2\uff0c\u7ecf\u97e9\u56fd\u8f6c\u673a\uff0c\u53ef\u4eab\u6700\u591a30\u5929\u514d\u7b7e\u5165\u5883\u7684\u4f18\u60e0\uff0c\u8fd9\u5438\u5f15\u4e86\u5927\u6279\u4e2d\u56fd\u6e38\u5ba2\u8d2d\u4e70\u97e9\u56fd\u822a\u7a7a\u516c\u53f8\u7684\u673a\u7968\uff0c\u7ecf\u9996\u5c14\u548c\u91dc\u5c71\u4e2d\u8f6c\u8d74\u7f8e\u3002',
	               u'\u822a\u7a7a\u754c\u8d44\u6df1\u4eba\u58eb\u8868\u793a\uff0c\u76f8\u5bf9\u4f4e\u8c03\u7684\u97e9\u4e9a\u822a\u7a7a\u5176\u5b9e\u662f\u901a\u8fc7\u8d27\u8fd0\u4ee3\u7406\u3001\u9500\u552e\u4ee3\u7406\u7b49\u5f62\u5f0f\u628a\u7f51\u70b9\u94fa\u5f00\u7684\uff0c\u4ea6\u6709\u975e\u5e38\u65b9\u4fbf\u4e14\u5148\u8fdb\u7684\u4fe1\u606f\u67e5\u8be2\u7cfb\u7edf\u3002',
	               u'\u201c\u97e9\u4e9a\u822a\u7a7a\u7684\u4f18\u52bf\u4e00\u4e2a\u662f\u4ef7\u683c\uff0c\u53e6\u4e00\u4e2a\u662f\u4e2d\u8f6c\u3002\u201d\u76ee\u524d\u97e9\u4e9a\u5df2\u5c06\u4e00\u7ebf\u5927\u57ce\u5e02\u57fa\u672c\u4e0a\u6709\u80fd\u529b\u505a',
	               u'\u65c5\u884c\u793e',
	               u'\u7684\u516c\u53f8\u201c\u5403\u4e86\u4e00\u904d\u201d\uff0c\u73b0\u5728\u7684\u9500\u552e\u5e02\u573a\u5df2\u7ecf\u4ece\u4e00\u7ebf\u5e02\u573a\u8513\u5ef6\u5230\u4e86\u4e8c\u7ebf\u57ce\u5e02\uff0c\u751a\u81f3\u662f\u4e09\u7ebf\u57ce\u5e02\u3002',
	               u'\u56e0\u4e3a\u6709\u4ef7\u683c\u4f18\u52bf\u548c\u65b9\u4fbf\u5feb\u6377\u7684\u67e5\u8be2\u7cfb\u7edf\uff0c\u97e9\u4e9a\u822a\u7a7a\u7684\u5ba2\u6237\u5927\u90e8\u5206\u662f\u975e\u5546\u52a1\u5ba2\uff0c\u57fa\u672c\u662f\u51fa\u6e38\u548c\u7559\u5b66\u751f\u56e2\u4f53\uff0c\u7559\u5b66\u751f\u5927\u90e8\u5206\u56fe\u4fbf\u5b9c\u4e0d\u5728\u4e4e\u65f6\u95f4\uff0c\u800c',
	               u'\u65c5\u6e38',
	               u'\u7684\u5ba2\u6237\u5927\u90e8\u5206\u662f\u7b2c\u4e00\u6b21\u51fa\u56fd\uff0c\u6216\u8005\u51fa\u56fd\u6b21\u6570\u4e0d\u591a\uff0c\u589e\u52a0\u4e00\u4e2a\u4e2d\u8f6c\u70b9\uff0c\u4ee5\u4e3a\u53ef\u4ee5\u5f00\u5f00\u773c\u754c\uff0c\u5e76\u4e0d\u89c9\u5f97\u6d6a\u8d39\u65f6\u95f4\u3002',
	               u'\u201c\u5b9e\u9645\u4e0a\uff0c\u97e9\u4e9a\u822a\u7a7a\u73b0\u5728\u5728\u589e\u957f\u7684(\u4e1a\u52a1)\uff0c\u90fd\u662f\u4e2d\u56fd\u7684\u5e73\u6c11\u6216\u8005\u521a\u5bcc\u88d5\u8d77\u6765\u7684\u5e73\u6c11\uff0c\u8fd9\u5757\u5e02\u573a\u672a\u6765\u7684\u589e\u957f\u6f5c\u529b\u975e\u5e38\u5927\uff0c\u73b0\u5728\u97e9\u56fd\u7684\u822a\u7a7a\u516c\u53f8\u57fa\u672c\u4e0a\u5728\u8fd9\u5757\u5e02\u573a\u7684\u6e20\u9053\u5df2\u7ecf\u5efa\u7acb\u5b8c\u6574\u4e86\u3002\u201d\u4e0a\u8ff0\u4e1a\u5185\u4eba\u58eb\u8868\u793a\u3002',
	               u'\u97e9\u4e9a\u62bc\u5b9d\u4e2d\u56fd\u5e02\u573a',
	               u'\u8fd1\u5341\u5e74\u6765\uff0c\u5305\u62ec\u97e9\u4e9a\u3001\u5927\u97e9\u5728\u5185\u7684\u97e9\u56fd\u822a\u7a7a\u516c\u53f8\u5728\u4e2d\u56fd\u5e02\u573a\u4e0a\uff0c\u822a\u7ebf\u3001\u822a\u73ed\u4e00\u76f4\u5728\u5f00\u62d3\u6269\u5f20\uff0c\u97e9\u56fd\u9996\u5c14\u4e5f\u56e0\u6cbe\u4e2d\u56fd\u5185\u5730\u56fd\u9645\u822a\u73ed\u5e02\u573a\u7e41\u8363\u7684\u5149\uff0c\u6210\u4e3a\u4e16\u754c\u4e0a\u4e00\u4e2a\u91cd\u8981\u7684\u822a\u7a7a\u67a2\u7ebd\u548c\u975e\u5e38\u96c6\u4e2d\u7684\u822a\u7a7a\u5ba2\u8d27\u8fd0\u4e2d\u8f6c\u70b9\u3002',
	               u'\u4e2d\u56fd\u548c\u65e5\u672c\u5206\u522b\u662f\u97e9\u4e9a\u822a\u7a7a\u5168\u7403\u7684\u6700\u5927\u5e02\u573a\u53ca\u6700\u4e3b\u8981\u5229\u6da6\u6765\u6e90\u3002\u97e9\u4e9a\u822a\u7a7aCEO\u5c39\u6c38\u6597\u65e5\u524d\u8868\u793a\uff0c\u201c\u4e2d\u56fd\u3001\u65e5\u672c\u662f\u97e9\u4e9a\u822a\u7a7a\u7684\u6218\u7565\u822a\u7ebf\uff0c\u5c24\u5176\u662f\u4e2d\u56fd\u5e02\u573a\uff0c\u8fd8\u62e5\u6709\u6781\u5927\u7684\u589e\u957f\u7a7a\u95f4\u3002\u201d\u636e\u5c39\u6c38\u6597\u4ecb\u7ecd\uff0c\u97e9\u65e5\u822a\u7ebf\u4e4b\u6240\u4ee5\u5229\u6da6\u5360\u4f18\uff0c\u4e3b\u8981\u5728\u4e8e\u822a\u7ebf\u8ddd\u79bb\u4e0e\u7968\u9762\u4ef7\u683c\uff0c\u5c31\u5e02\u573a\u6f5c\u529b\u548c\u589e\u901f\u6765\u770b\uff0c\u4e2d\u56fd\u4ecd\u662f\u97e9\u4e9a\u822a\u7a7a\u62bc\u5b9d\u7684\u91cd\u70b9\u3002',
	               u'\u636e\u6089\uff0c\u97e9\u4e9a\u822a\u7a7a\u81ea1994\u5e74\u5f00\u901a\u5317\u4eac\u3001\u4e0a\u6d77\u822a\u7ebf\u4ee5\u6765\uff0c\u622a\u81f3\u76ee\u524d\u5df2\u5728\u4e2d\u56fd21\u4e2a\u57ce\u5e02\u5f00\u901a\u4e8630\u6761\u822a\u7ebf\uff0c\u6bcf\u5468\u5f80\u8fd4\u4e2d\u56fd210\u4e2a\u822a\u73ed\uff0c\u4e0d\u4ec5\u662f\u5f80\u6765\u4e2d\u97e9\u4e4b\u95f4\u822a\u7ebf\u6700\u5bc6\u96c6\u7684\u822a\u7a7a\u516c\u53f8\uff0c\u4e5f\u662f\u5728\u4e2d\u56fd\u5883\u5185\u62e5\u6709\u6700\u591a\u822a\u7ebf\u7684\u5916\u56fd\u822a\u7a7a\u516c\u53f8\u3002',
	               u'\u4e8b\u5b9e\u4e0a\uff0c\u6781\u5bcc\u589e\u957f\u6027\u548c\u8bf1\u60d1\u529b\u7684\u4e2d\u56fd\u5e02\u573a\uff0c\u76ee\u524d\u5df2\u6210\u4e3a\u5168\u7403\u5404\u822a\u7a7a\u516c\u53f8\u7ade\u76f8\u5e03\u5c40\u7684\u76ee\u6807\u3002\u4eca\u5e746\u67084\u65e5\uff0c\u56fd\u9645\u822a\u534f\u9884\u6d4b\uff0c2013\u5e74\uff0c\u4e9a\u592a\u5730\u533a\u822a\u7a7a\u516c\u53f8\u5c06\u5b9e\u73b046\u4ebf\u7f8e\u5143\u7684\u5408\u5e76\u5229\u6da6  (\u9ad8\u4e8e\u5148\u524d42\u4ebf\u7f8e\u5143\u7684\u9884\u671f)\u3002\u4e0d\u8bba\u5728\u7edd\u5bf9\u5229\u6da6\u8fd8\u662f\u5728\u606f\u7a0e\u524d\u5229\u6da6\u7387\u65b9\u9762\uff0c\u8be5\u5730\u533a\u90fd\u5c06\u9886\u5148\u4e8e\u5176\u4ed6\u6240\u6709\u5730\u533a\uff0c\u5176\u4e2d\u4e2d\u56fd\u5e02\u573a\u5f3a\u52b2\u589e\u957f\u662f\u4e3b\u8981\u63a8\u52a8\u56e0\u7d20\u3002\u4e9a\u592a\u822a\u7a7a\u516c\u53f8\u603b\u5ba2\u8fd0\u9700\u6c42\u9884\u8ba1\u5c06\u589e\u957f6.3%\uff0c\u9886\u5148\u4e8e5.6%\u7684\u8fd0\u529b\u589e\u957f\u3002'],
