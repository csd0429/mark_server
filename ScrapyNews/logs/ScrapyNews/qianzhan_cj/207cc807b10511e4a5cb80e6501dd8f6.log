	               u'\u5168\u56fd\u201c\u626b\u9ec4\u6253\u975e\u201d\u529e\u516c\u5ba4\u4f20\u51fa\u6d88\u606f\uff0c8\u67088\u65e5\uff0c\u6df1\u5733\u5feb\u64ad\u79d1\u6280\u6709\u9650\u516c\u53f8\u7f51\u4e0a\u4f20\u64ad\u6deb\u79fd\u8272\u60c5\u4fe1\u606f\u6848\u4e3b\u8981\u72af\u7f6a\u5acc\u7591\u4eba\u3001\u5feb\u64ad\u516c\u53f8\u6cd5\u4eba\u4ee3\u8868\u517c\u603b\u7ecf\u7406\u738b\u6b23\uff0c\u5728\u9003\u5f80\u5883\u5916110\u5929\u540e\u88ab\u6293\u6355\u5f52\u6848\uff0c\u7ecf\u56fd\u9645\u53f8\u6cd5\u5408\u4f5c\u6e20\u9053\u7531\u76f8\u5173\u56fd\u5bb6\u79fb\u4ea4\u4e2d\u56fd\u8b66\u65b9\u3002\u7ecf\u5ba1\u8baf\uff0c\u738b\u6b23\u5bf9\u660e\u77e5\u5feb\u64ad\u516c\u53f8',
	               u'\u670d\u52a1\u5668',
	               u'\u5185\u6709\u5927\u91cf\u6deb\u79fd\u8272\u60c5\u89c6\u9891\uff0c\u4e3a\u4e86\u725f\u5229\u653e\u4efb\u4e0d\u7ba1\u7684\u72af\u7f6a\u4e8b\u5b9e\u4f9b\u8ba4\u4e0d\u8bb3\u3002\u76ee\u524d\uff0c\u8be5\u6848\u6b63\u5728\u8fdb\u4e00\u6b65\u67e5\u529e\u4e2d\u3002',
	               u'\u636e\u6089\uff0c\u81ea4\u6708\u4ee5\u6765\uff0c\u5feb\u64ad\u516c\u53f8\u7684\u5f88\u591a\u5458\u5de5\u9646\u7eed\u79bb\u804c\uff0c\u76ee\u524d\u4f4d\u4e8e\u6df1\u5733\u5357\u5c71\u79d1\u6280\u56ed\u7684\u5feb\u64ad\u603b\u90e8\u5df2\u7ecf\u4eba\u53bb\u697c\u7a7a\uff0c\u539f\u5740\u5df2\u6613\u4e3b\u3002\u6b64\u524d\uff0c\u521b\u59cb\u4eba\u738b\u6b23\u4e00\u76f4\u5904\u4e8e\u5931\u8054\u72b6\u6001\u3002\u800c\u5feb\u64ad\u79d1\u6280\u5b98\u65b9\u5fae\u535a\u6700\u65b0\u6d88\u606f\u7684\u53d1\u5e03\u65f6\u95f4\u57285\u670819\u65e5\u3002',
