	 'news_info': [u'\u8fd1\u65e5\u56fd\u5185\u591a\u5bb6\u822a\u7a7a\u516c\u53f8\u5ba3\u5e03\uff0c\u51fa\u7968\u65e5\u671f\u57282\u67085\u65e5\u6216\u4ee5\u540e\u7684\u56fd\u5185\u822a\u7ebf\u673a\u7968\uff0c\u5c06\u5168\u90e8\u53d6\u6d88\u71c3\u6cb9\u9644\u52a0\u8d39\uff0c\u65c5\u5ba2\u53ea\u9700\u652f\u4ed8\u673a\u7968\u8d39\u7528\u4e0e50\u5143\u6c11\u822a\u53d1\u5c55\u57fa\u91d1\u5373\u53ef\u51fa\u884c\u3002\u8fd9\u662f2009\u5e74\u6765\u56fd\u5185\u5404\u5927\u822a\u7a7a\u516c\u53f8\u7b2c\u4e8c\u6b21\u505c\u6536\u71c3\u6cb9\u9644\u52a0\u8d39\u3002',
	               u'2\u67085\u65e5\u8d77\uff0c\u71c3\u6cb9\u9644\u52a0\u8d39\u5c06\u5168\u90e8\u53d6\u6d88',
	               u'\u53c8\u662f\u4e00\u8f6e\u6cb9\u4ef7\u4e0b\u884c\u671f\uff0c\u6d88\u8d39\u8005\u4e5f\u8fce\u6765\u4e86\u71c3\u6cb9\u9644\u52a0\u8d39\u7684\u7b2c\u4e8c\u6b21\u514d\u5f81\u3002\u6b64\u524d\uff0c\u56fd\u5185\u822a\u7ebf\u71c3\u6cb9\u9644\u52a0\u8d39\u5df2\u8fde\u7eed5\u4e2a\u6708\u4e0b\u8c03\u3002\u4eca\u5e741\u67085\u65e5\u4e3a\u6700\u8fd1\u7684\u4e00\u6b21\uff0c\u4e0b\u8c03\u540e\uff0c800\u516c\u91cc\u4ee5\u4e0b\u822a\u6bb5\u5f81\u6536\u6807\u51c6\u4e3a10\u5143\uff0c800\u516c\u91cc\u4ee5\u4e0a\u822a\u7ebf\u5f81\u6536\u6807\u51c6\u4e3a30\u5143\u3002\u4e5f\u5c31\u662f\u8bf4\uff0c\u6b64\u6b21\u53d6\u6d88\u71c3\u6cb9\u9644\u52a0\u8d39\u53ef\u4e3a\u6d88\u8d39\u8005\u5e26\u676510\u201430\u5143\u7684\u4f18\u60e0\u3002',
	               u'\u6307\u671b\u4fbf\u5b9c\u673a\u7968\u8fc7\u5e74\u56de\u5bb6\u7684\u670b\u53cb\u4eec\u53ef\u4ee5\u4e00\u7b11\u800c\u8fc7\u4e86\u3002\u4e00\u6765\u6625\u8282\u671f\u95f4\u4f9b\u6c42\u5173\u7cfb\u51b3\u5b9a\u4e86\u673a\u7968\u4ef7\u683c\u4e0d\u53ef\u80fd\u6709\u5927\u7684\u6298\u6263;\u4e8c\u6765\uff0c\u56fd\u5185\u7684\u9ad8\u4ef7\u673a\u7968\u5e76\u4e0d\u662f\u6709\u4e86\u8fd930\u5143\u7684\u4f18\u60e0\u5c31\u53ef\u4ee5\u53d8\u5f97\u201c\u5e73\u6c11\u201d\u4e86\u7684\uff0c\u6253\u7834\u6c11\u7528\u822a\u7a7a\u4e1a\u7684\u5784\u65ad\uff0c\u6216\u8005\u81f3\u5c11\u8bf4\u591a\u5f15\u5165\u51e0\u4e2a\u5784\u65ad\u8005\uff0c\u624d\u662f\u5b9e\u73b0\u201c\u5e73\u6c11\u4ef7\u683c\u201d\u7684\u6839\u672c\u9014\u5f84\u3002',
	               u'\u6c11\u822a\u4e1a\u7684\u5784\u65ad\u5728\u6539\u9769\u5f00\u653e\u4e09\u5341\u591a\u5e74\u6765\u4f9d\u7136\u201c\u575a\u633a\u201d\u3002\u65e0\u8bba\u662f\u822a\u6cb9\u3001\u822a\u6750\uff0c\u8fd8\u662f\u5730\u9762\u670d\u52a1\uff0c\u5747\u6709\u5784\u65ad\u4f01\u4e1a\u628a\u63a7\uff0c\u5f53\u7136\u4e3a\u6b64\u57cb\u5355\u7684\uff0c\u4e0d\u4ec5\u4ec5\u662f\u6d88\u8d39\u8005\uff0c\u8fd8\u6709\u6c11\u822a\u4e1a\u81ea\u8eab\u7684\u53d1\u5c55\u3002',
	               u'2008\u5e74\u53cd\u5784\u65ad\u6cd5\u51fa\u53f0\u540e\uff0c\u6c11\u822a\u4e1a\u672c\u6765\u4e5f\u662f\u5728\u53cd\u5784\u65ad\u7684\u8303\u56f4\u4e4b\u7c7b\u3002\u4f46\u7531\u4e8e\u53d7\u91d1\u878d\u5371\u673a\u51b2\u51fb\uff0c\u6c11\u822a\u4e1a\u5229\u6da6\u4e0b\u964d\uff0c\u751a\u81f3\u51fa\u73b0\u8d1f\u5229\u6da6\uff0c\u6539\u9769\u4e0d\u5f97\u7684\u63a8\u540e\uff0c\u751a\u81f3\u6f14\u53d8\u4e3a\u4e86\u6682\u65f6\u4e0d\u6279\u51c6\u65b0\u7684\u822a\u7a7a\u516c\u53f8\u6210\u7acb\u4ee5\u633d\u6551\u6c11\u822a\u516c\u53f8\u7684\u201c\u4fc3\u5784\u65ad\u201d\u653f\u7b56\u3002\u6b64\u540e\uff0c\u53c8\u6709\u201c',
	               u'\u5b89\u5168',
	               u'\u201d\u5f53\u6321\u7bad\u724c\uff0c\u6c11\u822a\u7684\u53cd\u5784\u65ad\u6539\u9769\u6108\u53d1\u8fdf\u7f13\u3002\u98de\u673a\u8d2d\u4e70\u7531\u4e2d\u822a\u6750\u4e00\u624b\u628a\u63a7\uff0c\u822a\u5929\u7528\u6cb9\u7684\u8fdb\u53e3\u5219\u5fc5\u987b\u901a\u8fc7\u4e2d\u822a\u6cb9\uff0c\u4e09\u5bb6\u56fd\u6709\u822a\u7a7a\u516c\u53f8\u4e3b\u5bfc\u4e86\u5927\u90e8\u5206\u822a\u7ebf\u2026\u2026\u8fd9\u4e00\u7cfb\u5217\u5784\u65ad\u7684\u540e\u679c\u662f\uff0c\u98de\u673a\u91c7\u8d2d\u7684\u4ef7\u683c\u6700\u540e\u5f80\u5f80\u4e0d\u4f4e\uff0c\u4e2d\u56fd\u7684\u822a\u7a7a\u516c\u53f8\u5c11\u5219\u6700\u8d35\u7684\u822a\u6cb9\uff0c\u5730\u65b9\u822a\u7a7a\u548c\u6c11\u8425\u822a\u7a7a\u8981\u60f3\u83b7\u5f97\u822a\u7ebf\u5341\u5206\u56f0\u96be\u3002\u7f8a\u6bdb\u51fa\u5728\u7f8a\u8eab\u4e0a\uff0c\u4ee5\u4e0a\u6bcf\u4e00\u9053\u5784\u65ad\u6240\u589e\u52a0\u7684\u6210\u672c\uff0c\u90fd\u5c06\u53d8\u4e3a\u6d88\u8d39\u8005\u8d2d\u4e70\u673a\u7968\u65f6\u4ece\u94b1\u5305\u91cc\u638f\u51fa\u7684\u4eba\u6c11\u5e01\u3002',
	               u'\u53e6\u5916\uff0c\u56fd\u9645\u6cb9\u4ef7\u90fd\u5df2\u7ecf\u5927\u5e45\u4e0b\u964d\u534a\u5e74\u4e4b\u4e45\uff0c\u73b0\u5728\u624d\u53d6\u6d88\u71c3\u6cb9\u9644\u52a0\u8d39\u663e\u5f97\u6709\u4e9b\u8bda\u610f\u4e0d\u8db3\u3002\u4ece\u53e6\u4e00\u4e2a\u89d2\u5ea6\u6765\u8bf4\uff0c\u5982\u679c\u6cb9\u4ef7\u4e0a\u6da8\uff0c\u5784\u65ad\u8005\u4eec\u53ef\u4ee5\u9762\u4e0d\u6539\u8272\u5730\u5750\u6536\u9ad8\u4ef7\u673a\u7968\u548c\u71c3\u6cb9\u9644\u52a0\u8d39\u7684\u6536\u76ca\uff0c\u6cb9\u4ef7\u4e0b\u8dcc\u53cd\u800c\u4f7f\u4ed6\u4eec\u9762\u4e34\u8c03\u4ef7\u7684\u8206\u8bba\u538b\u529b\uff0c\u5f71\u54cd\u4e86\u5784\u65ad\u6536\u76ca\u3002\u8bf4\u4e0d\u5b9a\uff0c\u5784\u65ad\u8005\u4eec\u5e76\u4e0d\u559c\u6b22\u4e0b\u8dcc\u7684\u6cb9\u4ef7\u3002',
	               u'\u603b\u800c\u8a00\u4e4b\uff0c\u9ad8\u4ef7\u673a\u7968\u53d7\u8bdf\u75c5\u4e5f\u975e\u4e00\u65e5\u4e24\u65e5\uff0c\u501f\u7740\u6cb9\u4ef7\u4e0b\u8dcc\u671f\u7834\u9664\u822a\u7a7a\u5784\u65ad\u624d\u662f\u5b9e\u73b0\u822a\u7a7a\u4e1a\u5065\u5eb7\u53d1\u5c55\u548c\u6d88\u8d39\u8005\u798f\u5229\u7684\u6b63\u9053\u3002\u53d6\u6d88\u71c3\u6cb9\u9644\u52a0\u8d39\u53ea\u662f\u5c0f\u5934\uff0c\u800c\u4e14\u53ef\u80fd\u5728\u672a\u6765\u52a0\u56de\u6765\uff0c\u8fd9\u6837\u7684\u52a8\u4f5c\u662f\u4e0d\u80fd\u6ee1\u8db3\u5e02\u573a\u5bf9\u6539\u9769\u7684\u671f\u671b\u7684\u3002'],
