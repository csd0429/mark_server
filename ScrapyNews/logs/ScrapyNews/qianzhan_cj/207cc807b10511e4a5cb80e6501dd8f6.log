	               u'\u6e56\u5357\u623f\u5730\u4ea7\u4e0e\u5546\u4e1a\u4f1a\u51fa\u73b0\u6574\u4f53\u6298\u4ef7\u3002\u7b14\u8005\u624b\u8fb9\u6536\u5230\u7684\u4e00\u4efd\u6587\u4ef6\u663e\u793a\uff0c\u539f\u957f\u6c99\u94ec',
	               u'\u76d0',
	               u'\u538215\u4e07\u5e73\u65b9\u7c73\u7684\u5382\u533a\u5185\u5ca9\u571f\u3001\u5730\u8868\u6c34\u3001\u5730\u4e0b\u6c34\u5747\u5f62\u6210\u4e86\u6c61\u67d3\uff0c\u5f53\u5730\u5728\u6c61\u67d3\u5730\u533a\u5356\u5730\u9020\u623f\u5fc5\u987b\u4ed8\u51fa\u5de8\u5927\u7684\u6cbb\u7406\u6210\u672c\u3002\u9664\u975e\u5730\u65b9\u5b98\u5458\u4e27\u5931\u826f\u77e5\uff0c\u4e00\u58f0\u4e0d\u542d\u5356\u51fa\u9ad8\u4ef7\uff0c\u603b\u6709\u4e00\u5929\uff0c\u88ab\u4e27\u5931\u771f\u76f8\u7126\u8651\u7684\u8d2d\u623f\u8005\u4f1a\u96c6\u4f53\u6452\u5f03\u6e56\u5357\u6c5f\u8fb9\u7684\u623f\u4ea7\u3002',
	               u'\u8fdb\u4e00\u6b65\uff0c\u6e56\u5357\u4f1a\u88ab\u6295\u8d44\u8005\u629b\u5f03\u3002\u7edf\u8ba1\u6570\u636e\u663e\u793a\uff0c\u6e56\u5357\u5168\u7701\u53d7\u5230  \u201c\u77ff\u6bd2\u201d\u53ca\u91cd\u91d1\u5c5e\u6c61\u67d3\u7684\u571f\u5730\u9762\u79ef\u8fbe28000\u516c\u9877\uff0c\u5360\u5168\u7701\u603b\u9762\u79ef\u768413%\u3002\u6e56\u535714\u4e2a\u5e02\u3001\u5dde\u4e2d\uff0c\u67098\u4e2a\u5904\u5728\u6e58\u6c5f\u6d41\u57df\uff0c\u8d85\u8fc74000\u4e07\u4eba\u7684\u751f\u4ea7\u3001\u751f\u6d3b\u7528\u6c34\u53d7\u5230\u6c61\u67d3\uff0c\u8fd9\u4e00\u6d41\u57df\u5df2\u7ecf\u4e0d\u592a\u9002\u5408\u4eba\u5c45\uff0c\u4eba\u53e3\u4e0e\u8d44\u91d1\u80fd\u9003\u5219\u9003\uff0c\u9003\u5230\u6c61\u67d3\u4e0d\u90a3\u4e48\u4e25\u91cd\u7684\u5730\u533a\uff0c\u6e56\u5357\u5c06\u5149\u8363\u5730\u5b9e\u73b0\u628a\u6c61\u67d3\u7559\u7ed9\u81ea\u5df1\u3001\u628a\u8d22\u5bcc\u8f6c\u79fb\u51fa\u7701\u7684\u76ee\u6807\u3002',
	               u'\u6e56\u5357\u7684\u7ecf\u6d4e\u6210\u672c\u4f1a\u4e0a\u5347\u3002\u73b0\u5728\u6e56\u5357\u5927\u7c73\u6df1\u52a0\u5de5\u57fa\u5730\u76ca\u9633\u5170\u6eaa\u7c73\u5e02\u4e00\u5ea6\u670970%\u7684\u4f01\u4e1a\u5173\u95e8\uff0c\u519c\u6c11\u4e0d\u5b89\uff0c\u6c11\u751f\u4e0d\u7a33\uff0c\u6e56\u5357\u653f\u5e9c\u4f55\u4ee5\u7a33\u7ecf\u6d4e?',
	               u'\u6c61\u67d3\u6cbb\u7406\u6210\u672c\u4e0a\u5347\u3002\u4ee52011\u5e743\u6708\u56fd\u52a1\u9662\u6279\u590d\u7684  \u300a\u6e58\u6c5f\u6d41\u57df\u91cd\u91d1\u5c5e\u6c61\u67d3\u6cbb\u7406\u5b9e\u65bd\u65b9\u6848\u300b\u4e3a\u57fa\u7840\uff0c\u6e56\u5357\u4e0b\u53d1\u7684\u300a\u5de5\u4f5c\u65b9\u6848\u300b(2012~2015)\uff0c\u94c5\u3001\u9549\u7b49\u91cd\u91d1\u5c5e\u6392\u653e\u76ee\u6807\u4efb\u52a1\u4e0b\u8c03\u4e3a  \u201c\u57282008\u5e74\u7684\u57fa\u7840\u4e0a\u524a\u51cf50%\u201d\uff0c\u89c4\u5212\u9879\u76ee856\u4e2a\uff0c\u603b\u6295\u8d44505\u4ebf\u5143\u3002\u5176\u4e2d\u653f\u5e9c\u6295\u5165\u4e3a\u4e3b\u7684\u9879\u76ee\u5171225\u4e2a\u3001298\u4ebf\u5143\uff0c\u4f01\u4e1a\u6295\u5165\u4e3a\u4e3b\u7684\u9879\u76ee\u5171631\u4e2a\u3001207\u4ebf\u5143\uff0c\u8fd9\u662f\u5f53\u5730\u666e\u901a\u7eb3\u7a0e\u4eba\u4e0d\u53ef\u627f\u53d7\u4e4b\u91cd\uff0c\u8d22\u5bcc\u7531\u67d0\u4e9b\u7fa4\u4f53\u72ec\u4eab\uff0c\u6cbb\u7406\u7531\u653f\u5e9c\u4e70\u4e00\u534a\u7684\u5355\u30022013\u5e74\uff0c\u6e56\u5357\u6709\u8272\u91d1\u5c5e\u884c\u4e1a\u53bb\u5e74\u5b9e\u73b0\u5229\u7a0e238.4\u4ebf\u5143\uff0c\u5176\u4e2d\u5229\u6da6119.09\u4ebf\u5143\uff0c\u540c\u6bd4\u589e\u957f16.33%\u3002\u4e3a\u4e86\u533a\u533a\u767e\u4ebf\u5143\u7a0e\u6536\uff0c\u65ad\u7edd\u5b50\u5b59\u53d1\u5c55\u4e4b\u8def\uff0c\u53ef\u8c13\u611a\u8822\u800c\u77ed\u89c6\u3002\u8c01\u6c61\u67d3\uff0c\u8c01\u51fa\u94b1\u6cbb\u7406\uff0c\u5929\u7ecf\u5730\u4e49\u3002',
	               u'\u6e56\u5357\u516c\u5f00\u4fe1\u606f\u7684\u57fa\u7840\u662f\u52a0\u5f3a\u68c0\u6d4b\uff0c\u8fd9\u662f\u5206\u6563\u5f0f\u519c\u4e1a\u793e\u4f1a\u4e2d\u4fdd\u8bc1\u8d28\u91cf\u6ca1\u6709\u529e\u6cd5\u7684\u529e\u6cd5\u3002\u767e\u80dc\u96c6\u56e2\u88ab\u66dd\u5149\u65d7\u4e0b\u80af\u5fb7\u57fa\u9e21\u8089\u539f\u6599\u6709\u90e8\u5206',
	               u'\u6297\u751f\u7d20',
	               u'\u4e0d\u5408\u683c\uff0c\u767e\u80dc\u552f\u4e00\u7684\u529e\u6cd5\u662f\u51cf\u5c11\u5411\u5c0f\u519c\u6237\u8ba2\u8d2d\u91cf\uff0c\u52a0\u5f3a\u68c0\u6d4b\u3002',
	               u'\u6211\u56fd\u7684\u68c0\u6d4b\u6807\u51c6\u4e0e\u516c\u5e03\u7a0b\u5e8f\u6ede\u540e\u3002\u6211\u56fd\u7684\u571f\u58e4\u6c61\u67d3\u8c03\u67e5\u6570\u636e\u88ab\u73af\u4fdd\u90e8\u4ee5\u56fd\u5bb6\u673a\u5bc6\u4e3a\u7531\u62d2\u7edd\u516c\u5e03\uff0c\u957f\u671f\u4ee5\u6765\uff0c\u91cd\u91d1\u5c5e\u6ca1\u6709\u88ab\u5217\u5165\u7cae\u98df\u5e38\u89c4\u68c0\u6d4b\u9879\u76ee\uff0c\u56e0\u6b64\uff0c2002\u5e74\u52302008\u5e74\uff0c\u6ca1\u6709\u53d1\u73b0\u6e56\u5357\u5927\u7c73\u9549\u8d85\u6807\uff0c\u5305\u62ec\u4e2d\u50a8\u7cae\u4e5f\u4e0d\u505a\u536b\u751f\u6307\u6807\u7684\u68c0\u6d4b\u3002\u4e00\u4e9b\u6709\u68c0\u6d4b\u80fd\u529b\u7684\u5927\u578b\u4f01\u4e1a\u7684\u68c0\u6d4b\u6570\u636e\u53ea\u4f9b\u201c\u5185\u90e8\u53c2\u8003\u201d\u3002',
	               u'\u6e56\u5357\u5f53\u5730\u4e0d\u4ec5\u672a\u516c\u5f00\u68c0\u6d4b\u4fe1\u606f\uff0c\u5bf9\u4e8e\u5916\u5730\u7684\u68c0\u6d4b\u63ed\u4e11\u7684\u884c\u4e3a\u60f3\u5fc5\u5927\u4e3a\u5149\u706b\u3002\u4eca\u5e743\u6708\uff0c\u592e\u5e7f\u300a\u4e2d\u56fd\u4e4b\u58f0\u300b',
	               u'\u62a5',
	               u'\u9053\uff0c\u6e56\u5357\u65b9\u9762\u7cae\u5e93\u8d1f\u8d23\u4eba\u627f\u8ba4\u5927\u7c73\u91cd\u91d1\u5c5e\u8d85\u6807\u786e\u5b9e\u5c5e\u5b9e\uff0c\u4f46\u662f\u6e56\u5357\u65b9\u9762\u8868\u793a\uff0c\u6df1\u7cae\u96c6\u56e2\u68c0\u9a8c\u5927\u7c73\u91cd\u91d1\u5c5e\u542b\u91cf\uff0c\u5e76\u4e0d\u662f\u4e3a\u4e86\u7ef4\u62a4\u98df\u54c1\u536b\u751f\u5b89\u5168\u7684\u516c\u4f17\u5229\u76ca\uff0c\u800c\u662f\u4e3a\u4e86\u538b\u4f4e\u8fdb\u8d27\u4ef7\u800c\u4f7f\u7528\u7684\u624b\u6bb5\u3002\u6e56\u5357\u65b9\u9762\u4e0d\u4ec5\u4e0d\u611f\u8c22\u4ed6\u4eba\u7684\u68c0\u6d4b\uff0c\u53cd\u800c\u628a\u9ed1\u5e55\u89c6\u4f5c\u9634\u8c0b\u3002\u8fd9\u662f\u591a\u4e48\u4e0d\u81ea\u4fe1\uff0c\u624d\u80fd\u4ea7\u751f\u7684\u5fc3\u7406\u626d\u66f2\u3002',
	               u'\u6e56\u5357\u65b9\u9762\u9996\u5148\u8981\u516c\u5f00\u4fe1\u606f\uff0c\u540c\u65f6\u52a0\u5f3a\u5f53\u5730\u5927\u7c73\u7684\u91cd\u91d1\u5c5e\u68c0\u6d4b\uff0c\u5c06\u6e56\u5357\u5f53\u5730\u53d7\u5230\u6c61\u67d3\u7684\u571f\u5730\u79cd\u51fa\u7684\u7cae\u98df\u4e0e\u672a\u53d7\u6c61\u67d3\u7684\u9694\u7edd\uff0c\u4ee5\u589e\u5f3a\u6e56\u5357\u519c\u4ea7\u54c1\u4e43\u81f3\u653f\u5e9c\u7684\u6574\u4f53\u4fe1\u7528\uff0c\u8fd9\u662f\u51cf\u5c11\u6e56\u5357\u7ecf\u6d4e\u635f\u5931\u7684\u552f\u4e00\u529e\u6cd5\u3002\u5916\u677e\u5185\u7d27\uff0c\u5bf9\u5916\u4e0d\u9732\u4e1d\u6beb\u53e3\u98ce\uff0c\u5bf9\u5185\u52a0\u5f3a\u73af\u5883\u6cbb\u7406\uff0c\u8fd9\u79cd\u505a\u6cd5\u4e0d\u80fd\u5feb\u901f\u89c1\u6548\uff0c\u4e0d\u80fd\u6062\u590d\u5e02\u573a\u4fe1\u5fc3\uff0c\u65e0\u6cd5\u89e3\u51b3\u6cbb\u7406\u8d44\u91d1\u96be\u9898\u3002',
