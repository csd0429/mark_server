	               u'58253\u4e07\u4e0e1184\u4e07\u5143\u4e4b\u95f4\uff0c\u5b58\u5728\u7740\u600e\u6837\u7684\u8054\u7cfb?',
	               u'\u4eac\u4e1c\u7684\u8d22\u62a5\u663e\u793a\uff0c\u5728\u8ba1\u7b97\u975e\u4f1a\u8ba1\u901a\u7528\u51c6\u5219\u7684\u51c0\u5229\u6da6\u65f6\uff0c\u662f\u53bb\u6389\u4e86\u57fa\u4e8e\u80a1\u6743\u6fc0\u52b1\u768420893\u4e07\u5143\u548c\u57fa\u4e8e\u8d44\u4ea7\u548c\u4e1a\u52a1\u6536\u8d2d\u6240\u5bfc\u81f4\u7684\u65e0\u5f62\u8d44\u4ea7\u644a\u9500\u8d39\u7528\u768436176\u4e07\u5143\u3002\u800c\u76f8\u5bf9\u4e8e\u53bb\u5e74\u540c\u671f\u76846262\u4e07\u5143\u80a1\u6743\u6fc0\u52b1\uff0c\u672c\u5b63\u5ea6\u768420893\u4e07\u5143\u80a1\u6743\u6fc0\u52b1\u5927\u589e\u4e86234%\uff0c\u7edd\u5bf9\u503c\u4e5f\u589e\u957f\u4e8614631\u4e07\u5143\u3002',
	               u'\u8fd9\u5e76\u975e\u662f\u4e00\u4e2a\u53ef\u4ee5\u5ffd\u7565\u7684\u6570\u5b57\u3002',
	               u'\u9644\uff1a\u4eac\u4e1c\u4e8c\u5b63\u5ea6\u8d22\u62a5',
	               u'\u8d22\u62a5\u663e\u793a\uff0c\u4eac\u4e1c2014\u5e74\u7b2c\u4e8c\u5b63\u5ea6\u5b9e\u73b0\u51c0\u8425\u6536286\u4ebf\u4eba\u6c11\u5e01(\u7ea6\u540846\u4ebf\u7f8e\u5143)\uff0c\u540c\u6bd4\u589e\u957f64%;\u51c0\u4e8f\u635f5.825\u4ebf\u4eba\u6c11\u5e01(\u7ea6\u54089390\u4e07\u7f8e\u5143)\uff0c\u4e8f\u635f\u76f8\u6bd4\u53bb\u5e74\u540c\u671f\u76842830\u4e07\u4eba\u6c11\u5e01\u6269\u5927\u3002',
	               u'\u4eac\u4e1c\u7b2c\u4e8c\u5b63\u5ea6\u5546\u54c1\u4ea4\u6613\u603b\u989d(GMV)\u4e3a630\u4ebf\u4eba\u6c11\u5e01(\u7ea6\u5408102\u4ebf\u7f8e\u5143)\uff0c\u540c\u6bd4\u589e\u957f107%\u3002',
	               u'\u4eac\u4e1c\u7b2c\u4e8c\u5b63\u5ea6\u6d3b\u8dc3\u7528\u6237\u6570\u91cf\u4e3a3810\u4e07\uff0c\u76f8\u6bd4\u53bb\u5e74\u540c\u671f\u76841960\u4e07\u589e\u957f94%\u3002',
	               u'\u4eac\u4e1c\u7b2c\u4e8c\u5b63\u5ea6\u5c65\u884c\u7684\u8ba2\u5355\u91cf\u4e3a1.637\u4ebf\uff0c\u76f8\u6bd4\u53bb\u5e74\u540c\u671f\u76847260\u4e07\u589e\u957f126%\u3002\u5176\u4e2d\uff0c\u79fb\u52a8\u7aef\u8ba2\u5355\u5728\u603b\u8ba2\u5355\u91cf\u4e2d\u7684\u5360\u6bd4\u7ea6\u4e3a24%\u3002',
	               u'\u4eac\u4e1cCEO\u5218\u5f3a\u4e1c\u8868\u793a\uff0c\u201c\u4eac\u4e1c\u5b9e\u73b0\u4e86\u5f3a\u52b2\u7684\u5b63\u5ea6\u589e\u957f\uff0c\u8d85\u8fc7\u6211\u4eec\u7684\u76ee\u6807\uff0c\u53d6\u5f97\u4e86\u5e02\u573a\u4efd\u989d\u589e\u957f\u3002\u4f5c\u4e3a\u4e2d\u56fd\u5728\u7ebf',
	               u'\u76f4\u9500',
	               u'\u5e02\u573a\u7684\u9886\u5148\u8005\uff0c\u6211\u4eec\u5f88\u9ad8\u5174\u770b\u5230\u4eac\u4e1c\u6b63\u6210\u4e3a\u4e2d\u56fd\u6700\u53d7\u6b22\u8fce\u3001\u6700\u53ef\u4fe1\u7684\u6b63\u54c1\u8d2d\u4e70\u7f51\u7ad9\uff0c\u6211\u4eec\u8fd8\u62e5\u6709\u4e00\u6d41\u7684\u987e\u5ba2\u670d\u52a1\uff0c\u57fa\u4e8e\u81ea\u6709\u6700\u540e\u4e00\u516c\u91cc\u914d\u9001\u7f51\u7edc\u7684\u4fbf\u5229\u4e14\u5feb\u901f\u7684\u914d\u9001\u670d\u52a1\u3002\u201d',
	               u'\u4ed6\u8fd8\u8bf4\uff0c\u201c\u672a\u6765\uff0c\u6211\u4eec\u5c06\u4e13\u6ce8\u4e8e\u901a\u8fc7\u6269\u5927\u6211\u4eec\u5e73\u53f0\u7684\u4ea7\u54c1\u670d\u52a1\u54c1\u7c7b\uff0c\u4ee5\u53ca\u4f18\u5316\u6211\u4eec\u7684\u79fb\u52a8\u4ea7\u54c1\u548c\u6e17\u900f\u81f3\u5feb\u901f\u589e\u957f\u7684\u6b20\u53d1\u8fbe\u57ce\u5e02\uff0c\u6765\u5f3a\u5316\u4eac\u4e1c\u7684\u5e02\u573a\u9886\u5148\u5730\u4f4d\u3002\u6211\u4eec\u76f8\u4fe1\uff0c\u8fd9\u4e9b\u6218\u7565\u884c\u52a8\uff0c\u518d\u52a0\u4e0a\u4e2d\u56fd\u5e02\u573a\u6574\u4f53\u5448\u73b0\u51fa\u7684B2C\u7535\u5546\u8d8b\u52bf\uff0c\u5c06\u4e3a\u6211\u4eec\u7684\u957f\u671f\u589e\u957f\u6253\u4e0b\u575a\u5b9e\u57fa\u7840\u3002\u201d',
	               u'\u201c\u6211\u4eec\u5728\u7b2c\u4e8c\u5b63\u5ea6\u7684\u7a33\u5065\u8868\u73b0\u4f53\u73b0\u4e86\u6211\u4eec\u5f53\u524d\u63d0\u4f9b\u6700\u597d\u7684\u987e\u5ba2\u670d\u52a1\u548c\u63d0\u5347\u8fd0\u8425\u6548\u7387\u7684\u52aa\u529b\u3002\u201d\u4eac\u4e1c\u9996\u5e2d\u8d22\u52a1\u5b98\u9ec4\u5ba3\u5fb7\u8868\u793a\uff0c\u201c\u5728\u5f3a\u52b2\u7684\u8d44\u4ea7\u8d1f\u503a\u8868\u548c\u81ea\u7531\u73b0\u91d1\u6d41\u7684\u652f\u6301\u4e0b\uff0c\u6211\u4eec\u5c06\u7ee7\u7eed\u6218\u7565\u6295\u8d44',
	               u'\u7269\u6d41',
	               u'\u57fa\u7840\u8bbe\u65bd\u3001\u6280\u672f\u548c\u65b0\u4e1a\u52a1\u9879\u76ee\uff0c\u8fdb\u4e00\u6b65\u5f3a\u5316\u6211\u4eec\u5728\u4e2d\u56fd\u76f4\u9500B2C\u7535\u5546\u5e02\u573a\u7684\u9886\u5148\u5730\u4f4d\u3002\u201d',
	               u'\u7b2c\u4e8c\u5b63\u5ea6\u4e1a\u7ee9\u6458\u8981',
	               u'\u4eac\u4e1c\u7b2c\u4e8c\u5b63\u5ea6\u5546\u54c1\u4ea4\u6613\u603b\u989d(GMV)\u4e3a630\u4ebf\u4eba\u6c11\u5e01(\u7ea6\u5408102\u4ebf\u7f8e\u5143)\uff0c\u540c\u6bd4\u589e\u957f107%\u3002\u6765\u81ea\u5728\u7ebf\u76f4\u9500\u4e1a\u52a1\u548c\u5e02\u96c6\u4e1a\u52a1\u7684GMV\u5206\u522b\u4e3a392\u4ebf\u4eba\u6c11\u5e01\u548c238\u4ebf\u4eba\u6c11\u5e01\uff0c\u9ad8\u4e8e\u53bb\u5e74\u540c\u671f\u7684236\u4ebf\u4eba\u6c11\u5e01\u548c69\u4ebf\u4eba\u6c11\u5e01\u3002\u6765\u81ea\u7535\u5b50\u5bb6\u7535\u4ea7\u54c1\u7684GMV\u4e3a348\u4ebf\u4eba\u6c11\u5e01\uff0c\u9ad8\u4e8e\u53bb\u5e74\u540c\u671f\u7684200\u4ebf\u4eba\u6c11\u5e01;\u6765\u81ea\u65e5\u7528\u767e\u8d27\u548c\u5176\u5b83\u5546\u54c1\u7684GMV\u4e3a282\u4ebf\u4eba\u6c11\u5e01\uff0c\u9ad8\u4e8e\u53bb\u5e74\u540c\u671f\u7684105\u4ebf\u4eba\u6c11\u5e01\u3002\u65e5\u7528\u767e\u8d27\u548c\u5176\u5b83\u5546\u54c1\u7684GMV\u5360\u6bd4\u4e3a44.8%\uff0c\u9ad8\u4e8e\u53bb\u5e74\u540c\u671f\u768434.6%\u3002',
	               u'\u4eac\u4e1c\u7b2c\u4e8c\u5b63\u5ea6\u5b9e\u73b0\u51c0\u8425\u6536286\u4ebf\u4eba\u6c11\u5e01(\u7ea6\u540846\u4ebf\u7f8e\u5143)\uff0c\u540c\u6bd4\u589e\u957f64%\u3002GMV\u548c\u51c0\u8425\u6536\u7684\u589e\u957f\u4e3b\u8981\u56e0\u4e3a\u6d3b\u8dc3\u7528\u6237\u91cf\u7684\u589e\u957f\uff0c\u4ee5\u53ca\u8ba2\u5355\u91cf\u7684\u589e\u957f\u3002\u6765\u81ea\u5728\u7ebf\u76f4\u9500\u4e1a\u52a1\u7684\u51c0\u8425\u6536\u540c\u6bd4\u589e\u957f60%;\u6765\u81ea\u670d\u52a1\u4e0e\u5176\u5b83\u4e1a\u52a1\u7684\u51c0\u8425\u6536\u540c\u6bd4\u589e\u957f186%\uff0c\u589e\u957f\u4e3b\u8981\u53d7\u76ca\u4e8e\u8be5\u516c\u53f8\u5feb\u901f\u6269\u5f20\u7684\u5728\u7ebf\u5e73\u53f0\u548c',
	               u'\u5e7f\u544a',
	               u'\u670d\u52a1\u7684\u6536\u5165\u589e\u52a0\u3002',
	               u'\u8425\u4e1a\u6210\u672c\u4e3a255\u4ebf\u5143\u4eba\u6c11\u5e01\uff0c\u8f83\u53bb\u5e74\u540c\u671f\u7684159\u4ebf\u5143\u4eba\u6c11\u5e01\u9ad8\u51fa60%\u3002\u8fd9\u4e3b\u8981\u56e0\u4e3a\u8be5\u516c\u53f8\u7684\u76f4\u9500\u4e1a\u52a1\u589e\u957f\u3002',
	               u'\u7269\u6d41\u8d39\u7528\u4e3a20\u4ebf\u5143\u4eba\u6c11\u5e01\uff0c\u8f83\u53bb\u5e74\u540c\u671f\u768410\u4ebf\u5143\u4eba\u6c11\u5e01\u9ad8\u51fa105%\u3002',
	               u'\u8425\u9500\u8d39\u7528\u4e3a11\u4ebf\u5143\u4eba\u6c11\u5e01\uff0c\u8f83\u53bb\u5e74\u540c\u671f\u76844\u4ebf\u5143\u4eba\u6c11\u5e01\u9ad8\u51fa149%\u3002',
	               u'\u6280\u672f\u4e0e\u5185\u5bb9\u8d39\u7528\u4e3a4.2\u4ebf\u5143\u4eba\u6c11\u5e01\uff0c\u8f83\u53bb\u5e74\u540c\u671f\u76842.182\u4ebf\u5143\u4eba\u6c11\u5e01\u9ad8\u51fa92%\u3002',
	               u'\u4e00\u822c\u7ba1\u7406\u8d39\u7528\u4e3a4.554\u4ebf\u5143\u4eba\u6c11\u5e01\uff0c\u8f83\u53bb\u5e74\u540c\u671f\u76841.606\u4ebf\u5143\u4eba\u6c11\u5e01\u9ad8\u51fa184%\u3002',
	               u'\u4eac\u4e1c\u7b2c\u4e8c\u5b63\u5ea6\u51c0\u4e8f\u635f5.825\u4ebf\u5143\u4eba\u6c11\u5e01(\u7ea6\u54089390\u4e07\u7f8e\u5143)\uff0c\u9ad8\u4e8e\u53bb\u5e74\u540c\u671f\u76842830\u4e07\u5143\u4eba\u6c11\u5e01\u3002\u4e8f\u635f\u6269\u5927\u4e3b\u8981\u56e0\u4e3a\u4e0e\u817e\u8baf\u6218\u7565\u5408\u4f5c\u76f8\u5173\u7684\u8d44\u4ea7\u4e0e\u4e1a\u52a1\u6536\u8d2d\u5f15\u8d77\u7684\u65e0\u5f62\u8d44\u4ea7\u644a\u9500\u3002\u4e0d\u6309\u7f8e\u56fd\u901a\u7528\u4f1a\u8ba1\u51c6\u5219\uff0c\u4eac\u4e1c\u7b2c\u4e8c\u5b63\u5ea6\u51c0\u4e8f\u635f\u4e3a1180\u4e07\u5143\u4eba\u6c11\u5e01(\u7ea6\u5408190\u4e07\u7f8e\u5143)\uff0c\u800c\u53bb\u5e74\u540c\u671f\u4e3a\u76c8\u52293750\u4e07\u5143\u4eba\u6c11\u5e01\u3002',
	               u'\u4eac\u4e1c\u7b2c\u4e8c\u5b63\u5ea6\u6bcf\u80a1ADS\u51c0\u4e8f\u635f\u4e3a5.86\u5143\u4eba\u6c11\u5e01(\u7ea6\u54080.94\u7f8e\u5143)\uff0c\u9ad8\u4e8e\u53bb\u5e74\u540c\u671f\u76840.57\u5143\u4eba\u6c11\u5e01\u3002\u4e0d\u6309\u7f8e\u56fd\u901a\u7528\u4f1a\u8ba1\u51c6\u5219\uff0c\u6bcf\u80a1ADS\u51c0\u4e8f\u635f\u4e3a0.01\u5143\u4eba\u6c11\u5e01\uff0c\u800c\u53bb\u5e74\u540c\u671f\u4e3a\u6bcf\u80a1ADS\u76c8\u52290.04\u5143\u4eba\u6c11\u5e01\u3002',
	               u'\u73b0\u91d1\u6d41\u91cf\u4e0e\u8425\u8fd0\u8d44\u672c',
	               u'\u622a\u81f32014\u5e746\u670830\u65e5\uff0c\u4eac\u4e1c\u6301\u6709\u7684\u73b0\u91d1\u548c\u73b0\u91d1\u7b49\u4ef7\u7269\u3001\u53d7\u9650\u73b0\u91d1\u548c\u77ed\u671f\u6295\u8d44\u5171\u8ba1352\u4ebf\u5143\u4eba\u6c11\u5e01\uff0c\u9ad8\u4e8e\u622a\u81f32013\u5e7412\u670831\u65e5\u65f6\u7684146\u4ebf\u5143\u4eba\u6c11\u5e01\u3002',
	               u'2014\u5e74\u7b2c\u4e09\u5b63\u5ea6\u4e1a\u7ee9\u5c55\u671b',
	               u'\u4eac\u4e1c\u7b2c\u4e09\u5b63\u5ea6\u51c0\u8425\u6536\u9884\u8ba1\u5c06\u5728280\u4ebf\u5143\u4eba\u6c11\u5e01\u548c290\u4ebf\u5143\u4eba\u6c11\u5e01\u4e4b\u95f4\uff0c\u540c\u6bd4\u589e\u957f55%\u81f361%\u3002'],
