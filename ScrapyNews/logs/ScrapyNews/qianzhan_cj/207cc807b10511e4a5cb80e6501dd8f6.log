	               u'\u5229\u7387\u770b\u6da8\u524d\u666f\u4e0b\uff0c\u58a8\u897f\u54e5\u9700\u8981\u8c03\u6574\u516c\u5171\u652f\u51fa\uff0c\u51cf\u5c11\u6295\u8d44\u9879\u76ee\uff0c\u8fd9\u6b21\u9ad8\u94c1\u9879\u76ee\u6b63\u5305\u542b\u5728\u5176\u4e2d\u3002\u800c\u5728\u53bb\u5e7411\u6708\uff0c\u8fd9\u4e2a\u9879\u76ee\u66fe\u5728\u4e2d\u56fd\u4e2d\u6807\u540e\u77ed\u77ed\u4e09\u5929\u4e4b\u5185\u88ab\u53d6\u6d88\u3002\u8fd9\u4e2a\u672c\u5c06\u6210\u4e3a\u4e2d\u56fd\u9996\u4e2a\u6d77\u5916\u9ad8\u94c1\u9879\u76ee\u7684\u8ba1\u5212\u8fd9\u6b21\u771f\u7684\u9ec4\u4e86\u4e48?',
	               u'\u58a8\u897f\u54e5\u9ad8\u94c1\u9879\u76ee\u88ab\u65e0\u9650\u671f\u6401\u7f6e',
	               u'\u58a8\u897f\u54e5\u53c8\u53d8\u5366\u4e86\u3002',
	               u'\u4e0a\u6b21\u53d8\u5366\u662f\u7531\u4e8e\u5176\u4ed6\u6295\u6807\u4f01\u4e1a\u7684\u6297\u8bae\u4ee5\u53ca\u58a8\u897f\u54e5\u5728\u91ce\u515a\u7684\u53cd\u5bf9\u3002\u90a3\u4e9b\u9000\u51fa\u6295\u6807\u7684\u4f01\u4e1a\u79f0\uff0c\u4ece\u53d1\u6807\u5230\u622a\u6807\u4ec5\u7ecf\u5386\u4e862\u4e2a\u6708\uff0c\u62db\u6807\u65f6\u95f4\u8fc7\u77ed\uff0c\u5bf9\u4ed6\u4eec\u4e0d\u516c\u5e73\u3002\u5728\u91ce\u515a\u5219\u8ba4\u4e3a\u8fd9\u4e2a\u9879\u76ee\u7684\u9020\u4ef7\u8fd8\u662f\u592a\u9ad8\uff0c\u6307\u8d23\u653f\u5e9c\u62db\u6807\u4e0d\u900f\u660e\uff0c\u8fd8\u6697\u793a\u4e2d\u56fd\u4f01\u4e1a\u7684\u58a8\u897f\u54e5\u5408\u4f5c\u65b9\u548c\u653f\u5e9c\u6709\u5229\u76ca\u7275\u8fde\u3002\u62db\u6807\u56e0\u6b64\u53d6\u6d88\u3002',
	               u'\u90a3\u8fd9\u6b21\u53c8\u662f\u4ec0\u4e48\u7406\u7531?',
	               u'\u58a8\u897f\u54e5\u5c06\u8fd9\u6b21\u9ad8\u901f',
	               u'\u94c1\u8def',
	               u'\u9879\u76ee\u7684\u65e0\u9650\u671f\u6682\u505c\u5f52\u548e\u4e8e\u56fd\u9645\u7ecf\u6d4e\u5f62\u52bf\u3002\u77f3\u6cb9\u4ef7\u683c\u7684\u5927\u5e45\u4e0b\u8dcc\u4f7f\u4f5c\u4e3a\u77f3\u6cb9\u51fa\u53e3\u56fd\u7684\u58a8\u897f\u54e5\u8d22\u653f\u6536\u5165\u5927\u4e3a\u51cf\u5c11\u3002\u52a0\u4e0a\u7f8e\u56fd\u5229\u7387\u770b\u6da8\uff0c\u58a8\u897f\u54e5\u653f\u5e9c\u51b3\u5b9a\u5c06\u51cf\u5c11\u7ea6181\u4ebf\u6bd4\u7d22\u7684\u6295\u8d44\u9879\u76ee\uff0c\u8fd9\u6b21\u9ad8\u94c1\u9879\u76ee\u5f88\u4e0d\u5e78\u5c31\u5728\u5176\u4e2d\u3002\u5f53\u503c\u5f97\u4e00\u63d0\u7684\u662f\uff0c\u58a8\u897f\u54e5\u57ce\u65b0\u673a\u573a\u9879\u76ee\u3001\u8054\u90a6',
	               u'\u9ad8\u901f\u516c\u8def',
	               u'\u3001\u4e61\u6751\u516c\u8def\u548c\u76f4\u7ebf\u516c\u8def\u7684\u5efa\u8bbe\u9879\u76ee\u7b49\u5e76\u6ca1\u6709\u53d7\u5230\u5f71\u54cd\u3002\u8fd9\u79cd\u7406\u7531\u7684\u6a21\u7cca\u6027\u8ba9\u4eba\u6000\u7591\uff0c\u8fd9\u6b21\u9ad8\u94c1\u9879\u76ee\u53ef\u80fd\u771f\u7684\u9ec4\u4e86\u3002',
	               u'\u4e2d\u56fd\u505a\u9519\u4e86\u4ec0\u4e48\u8981\u906d\u9047\u5982\u6b64\u4e00\u800c\u518d\u7684\u201c\u53d8\u8138\u201d?\u5982\u679c\u8bf4\u7b2c\u4e00\u6b21\u53d8\u5366\u65f6\uff0c\u4e2d\u56fd\u5c1a\u6709\u4ee5\u5de8\u578b\u56fd\u4f01\u53c2\u6807\u800c\u6388\u4eba\u4ee5\u8bdd\u67c4\u4e4b\u5acc\uff0c\u90a3\u4e48\u7b2c\u4e8c\u6b21\u53d8\u5366\u4e2d\uff0c\u4e2d\u56fd\u771f\u7684\u662f\u201c\u8eba\u67aa\u201d\u4e86\u3002\u4e0d\u8fc7\u8fd9\u4e5f\u7ed9\u4e86\u6b63\u5728\u9010\u6e10\u8fc8\u5165\u201c\u8d70\u51fa\u53bb\u201d\u65f6\u4ee3\u7684\u4e2d\u56fd\u4f01\u4e1a\u4ee5\u67d0\u79cd\u8b66\u793a\u3002',
	               u'\u56fd\u9645\u9879\u76ee\u8981\u6210\u529f\uff0c\u4e0d\u4ec5\u4ec5\u8981\u6709\u81ea\u8eab\u786c\u4ef6\u7684\u5b8c\u5584\uff0c\u66f4\u8981\u66f4\u65b0\u201c\u8f6f\u4ef6\u201d\u6765\u9002\u5e94\u5f53\u5730\u7684\u5b9e\u9645\u60c5\u51b5\uff0c\u4e3a\u786c\u4ef6\u80fd\u591f\u53d1\u6325\u4f5c\u7528\u521b\u9020\u6761\u4ef6\u3002\u6bd4\u5982\u5728\u58a8\u897f\u54e5\uff0c\u56fd\u5185\u5728\u91ce\u515a\u7684\u653f\u6cbb\u538b\u529b\u548c\u5176\u653f\u5e9c\u4e00\u76f4\u9971\u53d7\u88d9\u5e26\u8d44\u672c\u4e3b\u4e49\u7684\u6307\u8d23\u4f7f\u5f97\u58a8\u897f\u54e5\u653f\u5e9c\u7684\u884c\u4e3a\u5fc5\u7136\u53d7\u5230\u67d0\u79cd\u9650\u5236\uff0c\u5c24\u5176\u5728\u4e2d\u56fd\u8fd9\u8fb9\u662f\u56fd\u4f01\u7684\u60c5\u51b5\u4e0b\uff0c\u66f4\u5bb9\u6613\u8ba9\u4eba\u6709\u53d1\u6325\u60f3\u8c61\u529b\u7684\u7a7a\u95f4\u3002\u6240\u6709\u8c23\u8a00\u7684\u57fa\u7840\u90fd\u662f\u4fe1\u606f\u4e0d\u5bf9\u79f0\u3002\u4e2d\u56fd\u5728\u53c2\u4e0e\u8fd9\u7c7b\u9879\u76ee\u65f6\uff0c\u867d\u7136\u81ea\u8eab\u6709\u4f18\u52bf\uff0c\u4f46\u4ecd\u987b\u4e0d\u65f6\u201c\u81ea\u8bc1\u5176\u6e05\u201d\uff0c\u624d\u80fd\u675c\u7edd\u8c23\u8a00\u3002\u53e6\u5916\uff0c\u56fd\u9645\u9879\u76ee\u53d7\u5168\u7403\u5b8f\u89c2\u7ecf\u6d4e\u7684\u5f71\u54cd\u66f4\u5927\uff0c\u8bc6\u522b\u597d\u8fd9\u7c7b\u98ce\u9669\u662f\u5316\u89e3\u98ce\u9669\u7684\u524d\u63d0\uff0c\u6709\u5907\u65e0\u60a3\uff0c\u4ee5\u514d\u7af9\u7bee\u6253\u6c34\u4e00\u573a\u7a7a\uff0c\u7ed9\u81ea\u5df1\u9020\u6210\u4e0d\u5fc5\u8981\u7684\u635f\u5931\u3002\u6bd4\u5982\u8fd9\u6b21\u53d8\u5366\u6240\u7ed9\u7406\u7531\u4e2d\u7684\u6cb9\u4ef7\u95ee\u9898\uff0c\u8fd9\u4e2a\u98ce\u9669\u672c\u5e94\u88ab\u8bc6\u522b\u3002',
	               u'\u4e0d\u8fc7\u4e5f\u6709\u89c2\u70b9\u8ba4\u4e3a\u6cb9\u4ef7\u4e0b\u8dcc\u5176\u5b9e\u4e0d\u8be5\u5bf9\u9ad8\u94c1\u9879\u76ee\u4ea7\u751f\u5f71\u54cd\u3002\u636e\u300a\u8bc1\u5238\u65e5\u62a5\u300b\u7684\u62a5\u9053\uff0c\u58a8\u897f\u54e5\u5f53\u5730',
	               u'\u62a5\u7eb8',
	               u'\u300a\u7ecf\u6d4e\u5b66\u5bb6\u62a5\u300b\u8ba4\u4e3a\u6cb9\u4ef7\u4e0b\u8dcc\u5176\u5b9e\u6b63\u662f\u63a8\u8fdb\u4ea4\u901a\u57fa\u7840\u8bbe\u65bd\u5efa\u8bbe\u7684\u597d\u65f6\u673a\uff0c\u4ed6\u4eec\u547c\u5401\u58a8\u897f\u54e5\u56de\u590d\u9ad8\u94c1\u9879\u76ee\u7684\u62db\u6807\u3002'],
